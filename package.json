{
  "name": "flatpage.usac.2018",
  "version": "1.0.0",
  "description": "Covering USAC Elections 2018",
  "main": "index.js",
  "repository": "http://github.com/daily-bruin/flatpage.usac.2018",
  "license": "MIT",
  "dependencies": {
    "react": "^15.0 || ^16.0",
    "react-dom": "^15.0 || ^16.0",
    "react-router-dom": "^4.2.2",
    "react-sanfona": "^1.2.2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.7.1",
    "prettier": "^1.12.1"
  },
  "scripts": {
    "format": "prettier --write \"{src,tests}/**/*.ts\" --single-quote --trailing-comma es5",
    "lint": "eslint --fix verbose \"src/**/*.js\"",
    "watch": "parcel index.html",
    "build": "parcel build index.html --public-url ./"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "yarn run lint",
      "yarn run format",
      "git add ."
    ]
  }
}
